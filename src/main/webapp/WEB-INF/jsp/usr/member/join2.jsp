<script>
	$(document).ready(function() {
		// AJAX로 다음 페이지 불러오기
		$('#next-button').click(function() {
			$.ajax({
				url : 'join_2.jsp', // 다음 화면을 불러올 경로
				method : 'GET',
				success : function(response) {
					$('.join-body').html(response); // join-body 부분만 업데이트
					// 히스토리 업데이트 (뒤로가기용)
					history.pushState({
						page : 'next'
					}, '', 'nextPage'); // URL을 nextPage로 변경
				},
				error : function() {
					alert('페이지를 불러오는 중 오류가 발생했습니다.');
				}
			});
		});

		window.onpopstate = function(event) {
			if (event.state && event.state.page === 'next') {
				// 뒤로 가기 시 이전 페이지를 다시 AJAX로 불러옴
				$.ajax({
					url : '/previousPage', // 이전 페이지 로드 경로
					method : 'GET',
					success : function(response) {
						$('.join-body').html(response);
					},
					error : function() {
						alert('이전 페이지를 불러오는 중 오류가 발생했습니다.');
					}
				});
			}
		};
	});
</script>

